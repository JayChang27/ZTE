class DirectedEdge
{
public:
	DirectedEdge(int v = 0, int w = 0, int weight = 0, int width = 0)
	{
		this->v = v;
		this->w = w;
		this->weight = weight;
		this->width = width;
	}
	~DirectedEdge()
	{

	}
	int from()
	{
		return v;
	}
	int to()
	{
		return w;
	}
	int Weight()
	{
		return weight;
	}
	int Width()
	{
		return width;
	}
private:
	int v;
	int w;
	int weight;
	int width;
};
